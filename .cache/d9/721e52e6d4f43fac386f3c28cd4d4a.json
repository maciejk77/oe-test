    onClick={handlePlusClick}\n                data-cy=\"plus\"\n              >\n                +\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <button className=\"item-price__add-button\">Add to cart</button>\n    </div>\n  );\n};\n\nexport default ItemPrice;\n"},"lineCount":null}},"error":null,"hash":"f6e2e8666d3abb842690114bf6cc0742","cacheData":{"env":{}}}