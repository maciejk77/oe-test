rem;\n    font-size: $font-larger-medium * 0.75;\n    line-height: 1.9rem;\n\n    &__header {\n      font-size: $font-big;\n    }\n  }\n}\n\n@media (min-width: $desktop) {\n  .item-specification {\n    &__header {\n      padding-bottom: 1.5rem;\n    }\n\n    &__specification {\n      //width: 80%;\n      margin: 0 auto;\n      margin-bottom: 2rem;\n      text-align: center;\n      width: 80%;\n    }\n\n    &__additionalSpecification {\n      text-align: center;\n    }\n\n    .list-item {\n      list-style-type: none;\n    }\n  }\n}\n","src/styles/variables.scss":"// colours\n$background-dark: #020d28;\n$background-light: #081a44;\n$text-default: #fff;\n$text-base: #92abd0;\n$button-base: #fd117a;\n$button-background: #35598d;\n\n// fonts\n$font-typeface: 'Montserrat';\n$font-small: 0.75rem;\n$font-medium: 1.2rem;\n$font-larger-medium: 1.75rem;\n$font-big: 2.25rem;\n\n// breakpoints\n$mobile-landscape: 576px;\n$tablet: 768px;\n$desktop: 992px;\n"},"lineCount":null}},"error":null,"hash":"640c31cb28aa41daf258e926af763efc","cacheData":{"env":{}}}